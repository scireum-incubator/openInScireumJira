{
  "manifest_version": 2,
  "name": "openInScireumJira",
  "short_name": "scireumJira",
  "description": "browses an issue on scireum jira",
  "version": "1",
  "minimum_chrome_version": "38",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "icons": {
    "16": "assets/jira.png"
  },
  "background": {
    "page": "background.html"
  }
  
}